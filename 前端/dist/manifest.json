{
  "name": "杉木搜索",
  "description" : "杉木搜索测试",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "static/img/favicon.ico"
  },
  "chrome_url_overrides": {
    "newtab": "newtap/index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}